}

<div class="text-center">
    <h1 class="display-4">Nova Cidade</h1>
</div>

<form method="post">
    <h6>Nome</h6>
    <input type="text" name="nome">
    <button type="submit"
            asp-route-ascii="@Model.ASCII">
        Enviar
    </button>
</form>